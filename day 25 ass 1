#include <stdio.h>

 main() 
 {
	int i,t;
	scanf("%d",&t);
	for(i=0;i<t;i++)
	{
	    long int n,i,sum=0;
	    scanf("%ld",&n);
	    long int a[n];
	    for(i=0;i<n;i++)
	    {
	        scanf("%ld",&a[i]);
	        sum+=a[i];
	    }
	    for(i=0;i<n;i++)
	    {
	        printf("%ld ",sum-a[i]);
	    }
	    printf("\n");
	}
	return 0;
}
