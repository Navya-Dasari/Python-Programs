#include<stdio.h>
main()
{

	int missingNumber(int* nums, int numsSize)
	{
        int nsum,i,sum=0;
        nsum=numsSize*(numsSize+1)/2;
        for(i=0;i<numsSize;i++)
        {
	        scanf("%d",&nums[i]);
	        for(i=0;i<numsSize;i++)
        	{
        		sum=sum+nums[i];
    		}
		}
        return nsum-sum;
	}
}
